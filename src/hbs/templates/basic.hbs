<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        <base href="{{env.base}}" >
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{{site.title}} - {{#if title}}{{title}} - {{/if}}{{site.title-info}}</title>
        <meta name="description" content="{{description}}">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="css/main.css">

        <script src="js/lib/html5shiv.min.js"></script>

        <script>
            (function(b,o,i,l,e,r){b.GoogleAnalyticsObject=l;b[l]||(b[l]=
            function(){(b[l].q=b[l].q||[]).push(arguments)});b[l].l=+new Date;
            e=o.createElement(i);r=o.getElementsByTagName(i)[0];
            e.src='//www.google-analytics.com/analytics.js';
            r.parentNode.insertBefore(e,r)}(window,document,'script','ga'));
            ga('create','UA-XXXXX-X');ga('send','pageview');
        </script>

    </head>
    <body>

        <header>

          <p class="site-title"><a href="/">{{site.title}}</a></p>
          <p class="intro">{{{site.intro}}}</p>

          <nav>
            <ul>
              {{#with site.nav.home}}
              <li {{#if active}}class="active"{{/if}}><a href="{{link}}">{{label}}</a></li>
              {{/with}}
              {{#with site.nav.contacts}}
              <li {{#if active}}class="active"{{/if}}><a href="{{link}}">{{label}}</a></li>
              {{/with}}
            </ul>
          </nav>

        </header>

        <div class="main">

          {{>body}}

        </div>

        <footer>
          <div>
            <div>&copy 2010-2014 DrabužiųTaisykla.lt</div>
            <div>{{site.credit}}<a href="http://arcsin.se/" target="_blank">Arcsin</a></div>
          </div>
        </footer>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="js/lib/jquery-1.11.1.min.js"><\/script>')</script>
        <script src="js/main.js"></script>

    </body>
</html>
